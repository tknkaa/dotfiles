{
  "agent": {
    "inline_assistant_model": {
      "provider": "copilot_chat",
      "model": "gpt-4.1"
    },
    "default_profile": "write",
    "default_model": {
      "provider": "google",
      "model": "gemini-2.5-pro"
    }
  },
  "show_edit_predictions": false,
  "buffer_font_size": 24,
  "theme": "Tokyo Night",
  "ui_font_size": 24,
  "vim_mode": true,
  "auto_install_extensions": {
    "Dracula": true,
    "dracula": true,
    "lua": true,
    "nix": true,
    "toml": true,
    "tokyonight": true,
    "Tokyo Night": true
  },
  "agent_font_size": 20,
  "relative_line_numbers": true,
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "chackOnSave": {
          "enable": false
        },
        "checkOnSave": {
          "enable": false
        },
        "settings": {
          "callInfo": {
            "enable": false
          },
          "cargo": {
            "runBuildScripts": false
          },
          "completion": {
            "enable": true
          },
          "diagnostics": {
            "enable": false
          },
          "hover": {
            "enable": true
          },
          "inlayHints": {
            "enable": false
          },
          "lens": {
            "enable": false
          },
          "procMacro": {
            "enable": false
          }
        }
      }
    }
  },
  "languages": {}
}
